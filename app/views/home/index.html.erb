<% content_for :title, "Dinify - Digital Menu Platform for Restaurants" %>

<!-- Header with Auth Options -->
<header class="bg-white shadow-sm border-b border-gray-200" data-controller="mobile-menu">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <div class="flex justify-between items-center h-16">
      <!-- Logo -->
      <div class="flex-shrink-0">
        <%= link_to root_path, class: "flex items-center" do %>
          <div class="h-8 w-8 bg-indigo-600 rounded-lg flex items-center justify-center">
            <span class="text-white font-bold text-lg">D</span>
          </div>
          <span class="ml-2 text-xl font-bold text-gray-900">Dinify</span>
        <% end %>
      </div>

      <!-- Desktop Navigation -->
      <div class="hidden md:block">
        <div class="ml-10 flex items-baseline space-x-8">
          <a href="#features" class="text-gray-600 hover:text-indigo-600 px-3 py-2 text-sm font-medium transition-colors">Features</a>
          <a href="#pricing" class="text-gray-600 hover:text-indigo-600 px-3 py-2 text-sm font-medium transition-colors">Pricing</a>
          <a href="#about" class="text-gray-600 hover:text-indigo-600 px-3 py-2 text-sm font-medium transition-colors">About</a>
          <a href="#contact" class="text-gray-600 hover:text-indigo-600 px-3 py-2 text-sm font-medium transition-colors">Contact</a>
        </div>
      </div>

      <!-- Auth Buttons -->
      <div class="hidden md:block">
        <div class="ml-4 flex items-center space-x-4">
          <% if authenticated? %>
            <%= link_to "Dashboard", dashboard_path, class: "text-gray-600 hover:text-indigo-600 px-3 py-2 text-sm font-medium transition-colors" %>
            <%= link_to "Sign Out", session_path(Current.session), method: :delete, 
                class: "bg-indigo-600 hover:bg-indigo-700 text-white px-4 py-2 rounded-md text-sm font-medium transition-colors",
                data: { turbo_method: :delete } %>
          <% else %>
            <%= link_to "Sign In", sign_in_path, class: "text-gray-600 hover:text-indigo-600 px-3 py-2 text-sm font-medium transition-colors" %>
            <%= link_to "Get Started", sign_up_path, class: "bg-indigo-600 hover:bg-indigo-700 text-white px-4 py-2 rounded-md text-sm font-medium transition-colors" %>
          <% end %>
        </div>
      </div>

      <!-- Mobile menu button -->
      <div class="md:hidden">
        <button type="button" 
                class="bg-white inline-flex items-center justify-center p-2 rounded-md text-gray-400 hover:text-gray-500 hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-indigo-500"
                data-action="click->mobile-menu#toggle">
          <span class="sr-only">Open main menu</span>
          <svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16" />
          </svg>
        </button>
      </div>
    </div>
  </div>

  <!-- Mobile Navigation -->
  <div class="hidden md:hidden" data-mobile-menu-target="menu">
    <div class="px-2 pt-2 pb-3 space-y-1 sm:px-3 bg-white border-t border-gray-200">
      <a href="#features" class="text-gray-600 hover:text-indigo-600 block px-3 py-2 text-base font-medium">Features</a>
      <a href="#pricing" class="text-gray-600 hover:text-indigo-600 block px-3 py-2 text-base font-medium">Pricing</a>
      <a href="#about" class="text-gray-600 hover:text-indigo-600 block px-3 py-2 text-base font-medium">About</a>
      <a href="#contact" class="text-gray-600 hover:text-indigo-600 block px-3 py-2 text-base font-medium">Contact</a>
      <div class="border-t border-gray-200 pt-4">
        <% if authenticated? %>
          <%= link_to "Dashboard", dashboard_path, class: "text-gray-600 hover:text-indigo-600 block px-3 py-2 text-base font-medium" %>
          <%= link_to "Sign Out", session_path(Current.session), method: :delete, 
              class: "bg-indigo-600 hover:bg-indigo-700 text-white block px-3 py-2 rounded-md text-base font-medium mx-3 mt-2",
              data: { turbo_method: :delete } %>
        <% else %>
          <%= link_to "Sign In", sign_in_path, class: "text-gray-600 hover:text-indigo-600 block px-3 py-2 text-base font-medium" %>
          <%= link_to "Get Started", sign_up_path, class: "bg-indigo-600 hover:bg-indigo-700 text-white block px-3 py-2 rounded-md text-base font-medium mx-3 mt-2" %>
        <% end %>
      </div>
    </div>
  </div>
</header>

<!-- Hero Section -->
<section class="bg-gradient-to-br from-indigo-50 via-white to-blue-50 py-20">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <div class="text-center">
      <h1 class="text-4xl md:text-6xl font-bold text-gray-900 mb-6">
        Digital Menus That
        <span class="text-indigo-600">Build Trust</span>
      </h1>
      <p class="text-xl text-gray-600 mb-8 max-w-3xl mx-auto">
        Create shareable digital menus with detailed nutritional information, allergy warnings, and multi-language support. 
        Perfect for restaurants, cafes, hotels, and food chains.
      </p>
      <div class="flex flex-col sm:flex-row gap-4 justify-center">
        <% if authenticated? %>
          <%= link_to "Go to Dashboard", dashboard_path, 
              class: "bg-indigo-600 hover:bg-indigo-700 text-white px-8 py-4 rounded-lg text-lg font-semibold transition-colors shadow-lg hover:shadow-xl" %>
          <%= link_to "View Demo", "#demo", 
              class: "border-2 border-indigo-600 text-indigo-600 hover:bg-indigo-600 hover:text-white px-8 py-4 rounded-lg text-lg font-semibold transition-colors" %>
        <% else %>
          <%= link_to "Start Free Trial", sign_up_path, 
              class: "bg-indigo-600 hover:bg-indigo-700 text-white px-8 py-4 rounded-lg text-lg font-semibold transition-colors shadow-lg hover:shadow-xl" %>
          <%= link_to "View Demo", "#demo", 
              class: "border-2 border-indigo-600 text-indigo-600 hover:bg-indigo-600 hover:text-white px-8 py-4 rounded-lg text-lg font-semibold transition-colors" %>
        <% end %>
      </div>
    </div>
  </div>
</section>

<!-- Features Section -->
<section id="features" class="py-20 bg-white">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <div class="text-center mb-16">
      <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-4">
        Everything You Need for Digital Menus
      </h2>
      <p class="text-xl text-gray-600 max-w-2xl mx-auto">
        Transform your restaurant's menu management with our comprehensive platform
      </p>
    </div>

    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
      <!-- Feature 1 -->
      <div class="bg-gray-50 p-8 rounded-xl hover:shadow-lg transition-shadow">
        <div class="w-12 h-12 bg-indigo-100 rounded-lg flex items-center justify-center mb-6">
          <svg class="w-6 h-6 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold text-gray-900 mb-4">Nutritional Information</h3>
        <p class="text-gray-600">
          Display detailed nutritional data including calories, protein, carbs, and allergens to build customer trust and meet health requirements.
        </p>
      </div>

      <!-- Feature 2 -->
      <div class="bg-gray-50 p-8 rounded-xl hover:shadow-lg transition-shadow">
        <div class="w-12 h-12 bg-indigo-100 rounded-lg flex items-center justify-center mb-6">
          <svg class="w-6 h-6 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold text-gray-900 mb-4">Shareable & QR Codes</h3>
        <p class="text-gray-600">
          Generate shareable links and QR codes for instant menu access. Perfect for contactless dining and social media sharing.
        </p>
      </div>

      <!-- Feature 3 -->
      <div class="bg-gray-50 p-8 rounded-xl hover:shadow-lg transition-shadow">
        <div class="w-12 h-12 bg-indigo-100 rounded-lg flex items-center justify-center mb-6">
          <svg class="w-6 h-6 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5h12M9 3v2m1.048 9.5A18.022 18.022 0 016.412 9m6.088 9h7M11 21l5-10 5 10M12.017 18l-3.5-7 3.5-7" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold text-gray-900 mb-4">Multi-Language Support</h3>
        <p class="text-gray-600">
          Support multiple languages including RTL languages like Arabic. Reach diverse customer bases with localized menus.
        </p>
      </div>

      <!-- Feature 4 -->
      <div class="bg-gray-50 p-8 rounded-xl hover:shadow-lg transition-shadow">
        <div class="w-12 h-12 bg-indigo-100 rounded-lg flex items-center justify-center mb-6">
          <svg class="w-6 h-6 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6V4m0 2a2 2 0 100 4m0-4a2 2 0 110 4m-6 8a2 2 0 100-4m0 4a2 2 0 100 4m0-4v2m0-6V4m6 6v10m6-2a2 2 0 100-4m0 4a2 2 0 100 4m0-4v2m0-6V4" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold text-gray-900 mb-4">Allergy Warnings</h3>
        <p class="text-gray-600">
          Clear allergy indicators and warnings for common allergens like gluten, nuts, dairy, and shellfish to ensure customer safety.
        </p>
      </div>

      <!-- Feature 5 -->
      <div class="bg-gray-50 p-8 rounded-xl hover:shadow-lg transition-shadow">
        <div class="w-12 h-12 bg-indigo-100 rounded-lg flex items-center justify-center mb-6">
          <svg class="w-6 h-6 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold text-gray-900 mb-4">Instant Updates</h3>
        <p class="text-gray-600">
          Update your menu in real-time and changes are instantly reflected across all shared links and QR codes.
        </p>
      </div>

      <!-- Feature 6 -->
      <div class="bg-gray-50 p-8 rounded-xl hover:shadow-lg transition-shadow">
        <div class="w-12 h-12 bg-indigo-100 rounded-lg flex items-center justify-center mb-6">
          <svg class="w-6 h-6 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold text-gray-900 mb-4">Analytics & Insights</h3>
        <p class="text-gray-600">
          Track menu performance, popular dishes, and customer preferences to optimize your offerings and increase sales.
        </p>
      </div>
    </div>
  </div>
</section>

<!-- How It Works Section -->
<section class="py-20 bg-gray-50">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
